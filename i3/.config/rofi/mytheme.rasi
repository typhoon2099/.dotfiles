* {
  white: #f1d2dc;
  pink: #dd95ad;
  dark-pink: #8d2e4d;
  dark: #251c1f;
  background-color: transparent;
}

window {
  background-color: @dark;
  border: 3px;
  border-color: @pink;
}

inputbar {
  padding: 5px;
  children: [ entry ];
}

entry {
  text-color: @pink;
}

element {
  padding: 5px;
  spacing: 5px;
  children: [element-icon, element-text];
}

element selected {
  background-color: @dark-pink;
}

element-text {
  text-color: @white;
  vertical-align: 0.5;
}

element-icon {
  size: 1.5em;
}

